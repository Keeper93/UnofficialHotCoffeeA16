		<li>
			<filter>
				<thingDefs>
					<li>RawCoffeeBeans</li>
				</thingDefs>
			</filter>
			<count>50</count>
		</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>RawCoffeeBeans</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<DecafCoffeeBeans>50</DecafCoffeeBeans>
		</products>
		<skillRequirements>
			<li>
				<skill>Cooking</skill>
				<minLevel>10</minLevel>
			</li>
		</skillRequirements>
		<workSkill>Cooking</workSkill>
		<researchPrerequisite>SweetDecafCoffee</researchPrerequisite>
	</RecipeDef>

</RecipeDefs>